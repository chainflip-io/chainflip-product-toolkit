{
  "name": "@chainflip/processor",
  "version": "2.1.1",
  "description": "",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chainflip/utils": "2.1.0-beta.1",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@chainflip/rpc": "1.9.3",
    "@polkadot/types": "^16.5.4"
  },
  "scripts": {
    "eslint:check": "pnpm eslint --max-warnings 0 './**/*.ts'",
    "prettier:base": "prettier ./** --ignore-path=../../.prettierignore",
    "prettier:check": "pnpm prettier:base --check",
    "prettier:write": "pnpm prettier:base --write",
    "test:ci": "CI=1 pnpm t run",
    "test": "vitest",
    "clean": "rm -rf dist",
    "prepublish": "pnpm test run && pnpm build",
    "build": "./build.sh"
  },
  "repository": "https://github.com/chainflip-io/chainflip-product-toolkit.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      },
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      }
    },
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  }
}
